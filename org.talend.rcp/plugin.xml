<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.talend.rcp.perspective">
         <actionSet
               id="org.talend.debug.ui.launchActionSet">
         </actionSet>
         <actionSet
               id="org.talend.repository.CreateactionSet">
         </actionSet>
         <actionSet
               id="org.talend.repository.actionSet">
         </actionSet>
         <actionSet
               id="org.talend.repository.localprovider.actionSet">
         </actionSet>
      </perspectiveExtension>
   </extension>
   
<extension
      point="org.eclipse.ui.commands">
   <category
         id="talend"
         name="Talend"/>
   <command
         id="org.talend.rcp.command.closeIntro"
         name="name">
   </command>
</extension>
<extension
      point="org.eclipse.ui.contexts">
   <context
         id="talend.global"
         name="Default"
         parentId="org.eclipse.ui.contexts.window"/>
</extension>
<extension
      point="org.talend.core.talendperspectives">
   <TalendPerspective
         refPerspectiveId="org.eclipse.debug.ui.DebugPerspective"
         talendPerspectiveId="org.talend.rcp.DebugPerspective"/>
</extension>
<extension
      point="org.talend.core.runtime.service">
   <Service
         class="org.talend.rcp.RcpServices"
         serviceId="IRcpService">
   </Service>
   <Service
         class="org.talend.rcp.intro.ApplicationWorkbenchWindowAdvisor"
         serviceId="WorkbenchService">
   </Service>
</extension>
<extension
      point="org.eclipse.ui.bindings">
   <key
         commandId="org.talend.rcp.command.closeIntro"
         schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
         sequence="ESC">
   </key>
</extension>
<extension
      point="org.eclipse.ui.editors">
   <editor
         class="org.talend.rcp.intro.starting.StartingBrowser"
         default="false"
         id="org.talend.rcp.intro.starting.StartingBrowser"
         name="starting">
   </editor>
</extension>
<extension
      point="org.eclipse.ui.activities">
   <activity
         id="org.talend.rcp.hideUpdatesite"
         name="Hide Updatesite">
   </activity>
   <activityPatternBinding
         activityId="org.talend.rcp.hideUpdatesite"
         isEqualityPattern="true"
         pattern="org.eclipse.ui.ide.application/org.eclipse.update.ui.updateMenu">
   </activityPatternBinding>
</extension>
<extension
      point="org.eclipse.ui.splashHandlers">
   <splashHandler
         class="org.talend.rcp.TalendSplashHandler"
         id="org.talend.rcp.splashHandler">
   </splashHandler>
</extension>
<extension
      point="org.eclipse.ui.commands">
   <command
         id="org.talend.rcp.show.exportLogs.command"
         name="Export Logs">
   </command>
</extension>
<extension
      point="org.eclipse.ui.handlers">
   <handler
         class="org.talend.rcp.handler.ShowExportLogsWizardHandler"
         commandId="org.talend.rcp.show.exportLogs.command">
   </handler>
</extension>
<extension
      point="org.eclipse.ui.menus">
   <menuContribution
         allPopups="false"
         locationURI="menu:help?after=talend">
      <command
            commandId="org.talend.rcp.show.exportLogs.command"
            id="org.talend.rcp.show.exportLogs.menus.command"
            style="push">
      </command>
   </menuContribution>
   <menuContribution
         allPopups="false"
         locationURI="menu:help?after=org.eclipse.equinox.p2.ui.sdk.install">
      <command
            commandId="org.eclipse.equinox.p2.ui.sdk.install"
            label="Install External Software..."
            style="push">
      </command>
   </menuContribution>
</extension>
<extension
      point="org.eclipse.ui.menus">
   <menuContribution
         allPopups="false"
         locationURI="menu:help?after=additions">
      <menu
            id="talend"
            label="talend">
      </menu>
   </menuContribution>
   <menuContribution
         allPopups="false"
         locationURI="menu:help?before=talend">
      <separator
            name="org.talend.help.spearator.start"
            visible="true">
      </separator>
   </menuContribution>
   <menuContribution
         allPopups="false"
         locationURI="menu:help?after=talend">
      <separator
            name="org.talend.help.spearator.end"
            visible="true">
      </separator>
   </menuContribution>
</extension>
<extension
      point="org.eclipse.ui.menus">
   <menuContribution
         allPopups="false"
         locationURI="menu:help?after=additions">
      <menu
            id="tdq"
            label="tdq">
      </menu>
   </menuContribution>
   <menuContribution
         allPopups="false"
         locationURI="menu:help?before=tdq">
      <separator
            name="org.talend.help.tdq.spearator.start"
            visible="true">
      </separator>
   </menuContribution>
   <menuContribution
         allPopups="false"
         locationURI="menu:help?after=tdq">
      <separator
            name="org.talend.help.tdq.spearator.end"
            visible="true">
      </separator>
   </menuContribution>
</extension>
<extension
      point="org.eclipse.ui.commands">
   <command
         id="org.talend.rcp.show.about.command"
         name="About Talend Studio">
   </command>
</extension>
<extension
      point="org.eclipse.ui.handlers">
   <handler
         class="org.talend.rcp.handler.ShowAboutWizardHandler"
         commandId="org.talend.rcp.show.about.command">
   </handler>
</extension>
<extension
      point="org.eclipse.ui.menus">
   <menuContribution
         allPopups="false"
         locationURI="menu:help?after=additions">
      <command
            commandId="org.talend.rcp.show.about.command"
            id="org.talend.rcp.show.about.menus.command"
            style="push">
      </command>
   </menuContribution>
</extension>
</plugin>
