<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="repositoryMavenSetting" name="RepositoryMavenSetting" schema="schema/repositoryMavenSetting.exsd"/>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.talend.designer.maven.ui.setting.project.initializer.MavenScriptsProjectSettingInitializer">
      </initializer>
   </extension>
   <extension
         point="org.talend.repository.projectsetting_page">
      <page
            class="org.talend.designer.maven.ui.setting.project.page.BuildProjectSettingPage"
            description="%ProjectSettingPage_BuildDesc"
            id="projectsetting.Build"
            title="%ProjectSettingPage_Build">
      </page>
      <page
            category="projectsetting.Build"
            class="org.talend.designer.maven.ui.setting.project.page.MavenProjectSettingPage"
            description="%ProjectSettingPage_MavenDesc"
            id="projectsetting.Maven"
            order="99"
            title="%ProjectSettingPage_Maven">
      </page>
      <page
            category="projectsetting.Maven"
            class="org.talend.designer.maven.ui.setting.project.page.MavenDefaultProjectSettingPage"
            description="%ProjectSettingPage_MavenDefaultDesc"
            id="projectsetting.MavenDefault"
            order="0"
            title="%ProjectSettingPage_MavenDefault">
      </page>
      <page
            category="projectsetting.Maven"
            class="org.talend.designer.maven.ui.setting.project.page.MavenCustomSetupProjectSettingPage"
            description="%ProjectSettingPage_MavenCustomSetupDesc"
            id="projectsetting.MavenCustomSetup"
            order="99"
            title="%ProjectSettingPage_MavenCustomSetup">
      </page>
      <page
            category="projectsetting.MavenDefault"
            class="org.talend.designer.maven.ui.setting.project.page.ProjectPomProjectSettingPage"
            description="%ProjectSettingPage_ProjectPomDesc"
            id="projectsetting.ProjectPom"
            order="0"
            title="%ProjectSettingPage_ProjectPom">
      </page>
   </extension>
   <extension
         point="org.talend.core.runtime.service">
      <Service
            class="org.talend.designer.maven.ui.MavenUIService"
            serviceId="IMavenUIService">
      </Service>
   </extension>
   <extension
         point="org.talend.core.repositoryContextualsActions">
      <Action
            class="org.talend.designer.maven.ui.actions.ConfigFolderLevelMavenPomAction"
            id="org.talend.designer.maven.ui.actions.ConfigFolderLevelMavenPomAction"
            isEditAction="false"
            isPropertiesAction="false"
            isReadAction="false"
            name="Config Folder Maven Pom">
      </Action>
   </extension>
   <extension
         point="org.talend.designer.maven.mavenSetting">
      <ProjectSettingManager
            class="org.talend.designer.maven.ui.setting.project.MavenUiProjectSettingManagerProvider">
      </ProjectSettingManager>
      <TemplateManager
            class="org.talend.designer.maven.ui.setting.MavenUiMavenTemplateManager">
      </TemplateManager>
   </extension>
   <extension
         point="org.talend.core.repository.login.task">
      <loginTask
            class="org.talend.designer.maven.ui.setting.preference.M2eUserSettingForTalendLoginTask"
            priority="highest">
      </loginTask>
   </extension>
   <extension
         point="org.talend.core.migrationTask">
      <projecttask
            beforeLogon="true"
            breaks="6.0.1"
            class="org.talend.designer.maven.ui.setting.migration.ConfigMavenRepositoryRemoveMigrationTask"
            description="Remove old Maven repository from configuration folder"
            id="org.talend.designer.maven.ui.ConfigMavenRepositoryRemove"
            name="RemoveConfigMavenRepository"
            version="6.0.2">
      </projecttask>
   </extension>

</plugin>
